{
  "name": "todo_list",
  "version": "1.0.0",
  "description": "Todo List CRUD application built with the MERN stack",
  "main": "server.js",
  "scripts": {
    "start": "node bin/www"
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "server": "nodemon bin/www",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Lionel Stewart",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "font-awesome": "^4.7.0",
    "mongoose": "^5.3.15"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
